/*
If the vaccine requires two doses, the vaccination date must be before today
*/

if (payload.v.0
    && payload.v.0.mp in [
        "EU/1/20/1528",
        "EU/1/20/1507",
        "EU/1/21/1529",
        "CoronaVac",
        "BBIBP-CorV",
        "Covishield",
        "Covaxin",
        "BBIBP-CorV_T",
        "CoronaVac_T",
        "Covaxin_T"
    ]) 
{
  /*
    dt: date of shot
  */
  (payload.v.0.dt as DateTime + 0#days) is not after (now() + 0#days)
} else {
  true
}