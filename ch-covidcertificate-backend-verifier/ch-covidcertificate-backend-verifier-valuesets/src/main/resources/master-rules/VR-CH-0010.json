  "certificateType": "Vaccination",
  "country": "CH",
  "description": [
    {
      "desc": "For vaccines approved for travellers in Switzerland, the validation date must be less than the vaccination date plus 365 days",
      "lang": "en"
    }
  ],
  "engine": "CertLogic",
  "engineVersion": "0.7.5",
  "identifier": "VR-CH-0010",
  "logic": {
    "if": [
      {
        "var": "payload.v.0"
      },
      {
        "if": [
          {
            "in": [
              {
                "var": "payload.v.0.mp"
              },
              [
                "PLACEHOLDER_TOURIST"
              ]
            ]
          },
          {
            "before": [
              {
                "plusTime": [
                  {
                    "var": "external.validationClock"
                  },
                  0,
                  "day"
                ]
              },
              {
                "plusTime": [
                  {
                    "var": "payload.v.0.dt"
                  },
                  365,
                  "day"
                ]
              }
            ]
          },
          true
        ]
      },
      true
    ]
  },
